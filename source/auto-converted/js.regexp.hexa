module js.regexp {
	@native('RegExp')
	declare class RegExp {
		var global: Bool
		var ignoreCase: Bool
		var multiline: Bool
		var source: String
		var lastIndex: Int
		function exec(str: String): js.regexp.RegExpMatch?
		function test(str: String): Bool
		function toString(): String
		new(pattern: String, flags: String?)
	}
}
