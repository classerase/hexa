module haxe.enumtools {
	declare class EnumValueTools {
		static function equals(a: haxe.enumtools.T, b: haxe.enumtools.T): Bool {
			return Type.enumEq(a, b)
		}

		static function getName(e: EnumValue): String {
			return e[0]
		}

		static function getParameters(e: EnumValue): [Any] {
			return e.slice(2)
		}

		static function getIndex(e: EnumValue): Int {
			return e[1]
		}

		static function match(e: EnumValue, pattern: Any): Bool
	}
}
