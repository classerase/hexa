module nodejs {
	@native('Buffer')
	declare class Buffer< nodejs.T > {
		var length: Int
		function readUInt16LE(offset: Int): Int
		function writeUInt16LE(value: Int, offset: Int): Int
		function readUInt32LE(offset: Int): Int
		function toString(encoding: String, start: Int?, end: Int?): String
		static function alloc(size: Int): nodejs.Buffer< nodejs.T >
		static function from(string: String, encoding: String?): nodejs.Buffer< nodejs.T >
	}
}
