module js.boot {
	private class HaxeError extends js.error.Error {
		private var val: Any
		@maybeUsed
		@has_untyped
		static function wrap(val: Any): Any {
			if(__instanceof__(val, js.error.Error)) return val else return new HaxeError(val)
		}

		new(val: Any) {
			super()
			this.val = __define_feature__('js.Boot.HaxeError', val)
			this.message = String(val)
			if(js.error.Error.captureStackTrace) js.error.Error.captureStackTrace(this, js.boot.HaxeError)
		}
	}
}
