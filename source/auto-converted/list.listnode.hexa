module list {
	private class ListNode< list.T > {
		var item: list.T
		var next: list.ListNode< list.T >
		@extern
		static function create(item: list.T, next: list.ListNode< list.T >): list.ListNode< list.T > {
			return new ListNode< list.T >(item, next)
		}

		new(item: list.T, next: list.ListNode< list.T >) {
			this.item = item
			this.next = next
		}
	}
}
