module list {
	private class ListNode< T > {
		var item: T
		var next: list.ListNode< T >
		@extern
		static function create(item: T, next: list.ListNode< T >): list.ListNode< T > {
			return new ListNode< T >(item, next)
		}

		new(item: T, next: list.ListNode< T >) {
			this.item = item
			this.next = next
		}
	}
}
