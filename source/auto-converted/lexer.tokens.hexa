module lexer {
	class Tokens {
		var token: nodejs.Buffer< Token >
		var value: [String]
		var length: Int
		var line: [Int]
		var column: [Int]
		var fileName: String
		new(tokens: nodejs.Buffer< Token >, length: Int, values: [String], lines: [Int], columns: [Int], fileName: String) {
			this.token = tokens
			this.length = length
			this.value = values
			this.line = lines
			this.column = columns
			this.fileName = fileName
		}
	}
}
